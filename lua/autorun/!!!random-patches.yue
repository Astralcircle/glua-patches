    Player = Player

    hook_Add "player_hurt", getHookName( "Player Decals Fix" ), ( data ) ->
        health = data.health
        if health > 0
            return

        ply = Player( data.userid )
        if IsValid( ply ) and Alive( ply )
            Simple 0.25, ->
                if IsValid( ply )
                    ply\RemoveAllDecals!

