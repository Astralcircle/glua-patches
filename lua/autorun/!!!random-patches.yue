                if HasFocus!
                    return

                lastNoFocusTime = CurTime!

        do

            :IsGameUIVisible, :ActivateGameUI, :HideGameUI = gui

            lastState = nil
            hook_Add "Think", getHookName( "False Screen Capture Fix" ), ->
                if HasFocus!
                    if lastState ~= nil
                        if lastState
                            HideGameUI!

                        lastState = nil

                elseif lastState == nil
                    lastState = not IsGameUIVisible!
                    if lastState
                        ActivateGameUI!

